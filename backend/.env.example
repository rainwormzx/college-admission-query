# 数据库连接URL (Railway会自动提供)
DATABASE_URL=postgresql://postgres:password@localhost:5432/admission_query

# 服务器配置
NODE_ENV=production
PORT=3000

# CORS配置 (允许的前端域名)
CORS_ORIGIN=http://localhost:5173,https://your-app.vercel.app

# 登录认证配置
ACCESS_PASSWORD=your_password_here
JWT_SECRET=your_jwt_secret_at_least_32_chars
JWT_EXPIRES_IN=24h

# 阿里云通义千问API配置
# 获取方式: https://dashscope.console.aliyun.com/apiKey
DASHSCOPE_API_KEY=your_dashscope_api_key_here
